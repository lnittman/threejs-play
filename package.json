{
  "name": "",
  "version": "0.0.0",
  "description": "<%= description %>",
  "license": "MIT",
  "repository": "https://github.com/<%= githubUsername %>/<%= title %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= website %>"
  },
  "scripts": {
    "postinstall": "yarn install-tree",
    "install-tree": "hash brew && ! hash tree && brew install tree || true",
    "start": "NODE_ENV=development webpack-dev-server",
    "prebuild": "yarn clean; yarn copy-public",
    "build": "NODE_ENV=production webpack",
    "clean": "rm -rf build",
    "copy-public": "cp -r public build",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "browserslist": {
    "production": [
      ">0.25%",
      "not ie 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 Chrome version",
      "last 1 Firefox version",
      "last 1 Safari version"
    ]
  },
  "dependencies": {
    "cannon": "^0.6.2",
    "canvas-sketch-util": "^1.10.0",
    "controls-gui": "^2.0.0",
    "controls-state": "^2.0.0",
    "datauritoblob": "^1.0.0",
    "detect-gpu": "^1.3.0",
    "glsl-vignette": "^1.1.0",
    "highest-power-two": "^1.0.2",
    "image-promise": "^7.0.1",
    "lodash": "^4.17.20",
    "orbit-controls": "^1.2.4",
    "p-map": "^4.0.0",
    "pretty-ms": "^7.0.0",
    "stats.js": "^0.17.0",
    "three": "0.125.0",
    "touches": "^1.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "babel-loader": "^8.1.0",
    "babel-preset-accurapp": "^5.1.0",
    "chalk": "^4.1.0",
    "detect-port": "^1.3.0",
    "eslint-config-accurapp": "^5.1.0",
    "event-hooks-webpack-plugin": "^2.1.7",
    "gh-pages": "^3.1.0",
    "glslify-loader": "^2.0.0",
    "html-webpack-plugin": "4.4.1",
    "indent-string": "^4.0.0",
    "raw-loader": "^4.0.1",
    "react-dev-utils": "^10.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
